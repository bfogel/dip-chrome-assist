{
    "compilerOptions": {
        "outDir": "./dist/",
        "module": "es2015",
        "noImplicitAny": true,
        "target": "es2015",
        "lib": [
            "es5",
            "es6",
            "es2017",
            "es2019",
            "dom"
        ],
        "strictNullChecks": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "sourceMap": true,
        "typeRoots": [
            "./node_modules/@types",
            "./typings"
        ],
    }
}